<!-- Stylesheet -->
<link href="../css/style.css" rel="stylesheet">
<!-- Custom Stylesheet -->
<link href="../css/custom.css" rel="stylesheet">

<div class="row cat-feed-info-area">
    <div class="col-lg-3 col-sm-12">
        <a href="#" class="btn-cat-feed-creat" data-toggle="modal" data-target="#exampleModal" data-whatever="@mdo">
            <div class="card cat-feed-info btn-feed-creat">
                급식소 만들기
            </div>
        </a>
        <div class="modal fade" id="exampleModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
            <div class="modal-dialog" role="document">
                <div class="modal-content">
                    <div class="modal-header">
                        <h5 class="modal-title" id="exampleModalLabel">급식소 만들기</h5>
                        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span>
                        </button>
                    </div>
                    <div class="modal-body">
                        <form action="/homes/index" method="get">
                            <div class="form-group">
                                <label for="recipient-name" class="col-form-label">급식소명</label>
                                <input type="text" class="form-control" id="recipient-name" name="v-cat-feed-name">
                            </div>
                            <div class="form-group">
                                <label for="message-text" class="col-form-label">급식소 정보</label>
                                <input type="text" class="form-control" id="recipient-name" name="v-cat-feed-info">
                            </div>
                            <div class="form-group">
                                <label for="message-text" class="col-form-label">운영 상태</label>
                                <input type="text" class="form-control" id="recipient-name" name="v-cat-feed-status">
                            </div>
                            <div class="form-group">
                                <label for="message-text" class="col-form-label">공개여부</label>
                                <input type="text" class="form-control" id="recipient-name" name="v-cat-feed-show">
                            </div>
                        </form>
                    </div>
                    <div class="modal-footer">
                        <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                        <button type="button" class="btn btn-primary">저장</button>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="col-lg-3 col-sm-12">
        <div class="card cat-feed-info">
            <!--<a href="#" class="btn-cat-feed-sch">-->
            <!--    <%= image_tag("cats/cat01.jpg", alt: "banner12", class: "img-fluid") %>-->
            <!--</a>-->
            <a class="cat-feed-info-des">
                <div class="cat-feed-info-title">
                    <h5 class="mt-3 mb-3">
                        <strong>#치즈네</strong>
                    </h5>
                </div>
                <div class="hastag-box">
                    <span class="hastag">#치즈냥</span>
                    <span class="hastag">#닭고기파</span>
                    <span class="hastag">#애기냥</span>
                </div>
                <div class="row">
                    <div class="col-6 border-right">
                        <div class="pt-3 pb-3 pl-0 pr-0 text-center">
                            <h4 class="m-1">운영중</h4>
                            <p class="m-0">상태</p>
                        </div>
                    </div>
                    <div class="col-6">
                        <div class="pt-3 pb-3 pl-0 pr-0 text-center">
                            <h4 class="m-1">비공개</h4>
                            <p class="m-0">공개여부</p>
                        </div>
                    </div>
                </div>
            </a>
        </div>
    </div>

</div>

